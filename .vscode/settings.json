{
  "nuxt.isNuxtApp": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "gitflow4code.init": {
    "develop": "develop",
    "master": "main",
    "releases": "release/",
    "hotfixes": "hotfix/",
    "features": "feature/",
    "support": ""
  },
  "gitflow4code.hotfixes": [],
  "yaml.schemas": {
    "https://www.artillery.io/schema.json": []
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.classAttributes": ["class", "ui"],
  "tailwindCSS.experimental.classRegex": [
    ["ui:\\s*{([^)]*)\\s*}", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ]
}
